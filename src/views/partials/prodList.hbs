<div class="container p-3">
  <h2 class="my-3">
    Listado de Productos:
  </h2>
  {{!-- {{#if productos}} --}}
  <table id="tablaProd" class="table table-striped table-hover" style="display: none">
    <thead>
      <tr>
        <th scope="col">
          #ID
        </th>
        <th scope="col">
          Title
        </th>
        <th scope="col">
          Price
        </th>
        <th scope="col">
          Image
        </th>
      </tr>
    </thead>
    <tbody>
      {{!-- {{#each productos}}
      {{> tableRaw}}
      {{/each}} --}}
    </tbody>
  </table>
  {{!-- {{else}} --}}
  <h3 id="noProd">
    No hay productos creados a√∫n
  </h3>
  {{!-- {{/if}} --}}
</div>
<script src="static/js/prodList.js"></script>